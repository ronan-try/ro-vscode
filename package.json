{
  "name": "ro-vscode",
  "displayName": "ro-vscode",
  "description": "vscode for ro-cli, but not limited",
  "version": "0.0.1",
  "publisher": "ronantry",
  "engines": {
    "vscode": "^1.46.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:ro-vscode.helloWorld",
    "onCommand:ro-vscode.mr",
    "onCommand:ro-vscode.start",
    "onCommand:ro-vscode.doRefactor"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "ro-vscode.helloWorld",
        "title": "Hello World",
        "category": "ro"
      },
      {
        "command": "ro-vscode.mr",
        "title": "Make a MR",
        "icon": "resources/icons/dark/ro.mr.png",
        "category": "ro"
      },
      {
        "command": "ro-vscode.start",
        "title": "start",
        "category": "ro"
      },
      {
        "command": "ro-vscode.doRefactor",
        "title": "dorefactor",
        "category": "ro"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "command": "ro-vscode.mr",
          "group": "navigation",
          "when": "config.git.enabled && !git.missing && gitOpenRepositoryCount != 0"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "test-compile": "tsc -p ./",
    "test-watch": "tsc -watch -p ./",
    "pretest": "npm run test-compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.46.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "ts-loader": "^8.0.14",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0",
    "webpack": "^5.19.0",
    "webpack-cli": "^4.4.0"
  },
  "dependencies": {
    "@ronan-try/cli-service": "^1.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ronan-try/ro-vscod.git"
  },
  "bugs": {
    "url": "https://github.com/ronan-try/ro-vscode/issues"
  },
  "homepage": "https://github.com/ronan-try/ro-vscode",
  "license": "MIT"
}
