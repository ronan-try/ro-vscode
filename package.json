{
  "name": "ro-vscode",
  "displayName": "ro-vscode",
  "description": "vscode for ro-cli, but not limited",
  "version": "0.0.2",
  "publisher": "ronantry",
  "engines": {
    "vscode": "^1.46.0"
  },
  "icon": "https://thumbnail0.baidupcs.com/thumbnail/877d6c930rfe4eddee7a1a5a3d86e164?fid=3407928918-250528-542425907987918&rt=pr&sign=FDTAER-DCb740ccc5511e5e8fedcff06b081203-clG7M4Jt%2fAHjqPu%2fzlLfJ9MfeAg%3d&expires=8h&chkbd=0&chkv=0&dp-logid=8864308675482619724&dp-callid=0&time=1620975600&size=c1920_u1080&quality=90&vuk=3407928918&ft=image&autopolicy=1",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:ro-vscode.helloWorld",
    "onCommand:ro-vscode.ui",
    "onCommand:ro-vscode.mr"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "ro-vscode.helloWorld",
        "title": "Hello World",
        "category": "ro"
      },
      {
        "command": "ro-vscode.ui",
        "title": "RoUI",
        "category": "ro"
      },
      {
        "command": "ro-vscode.mr",
        "title": "Make a MR/ ro mr",
        "icon": "resources/icons/dark/ro.mr.png",
        "category": "ro"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "command": "ro-vscode.mr",
          "group": "navigation",
          "when": "config.git.enabled && !git.missing && gitOpenRepositoryCount != 0"
        },
        {
          "command": "ro-vscode.ui",
          "group": "navigation",
          "when": "config.git.enabled && !git.missing && gitOpenRepositoryCount != 0"
        }
      ]
    }
  },
  "scripts": {
    "publish": "vsce publish",
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "test-compile": "tsc -p ./",
    "test-watch": "tsc -watch -p ./",
    "pretest": "npm run test-compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.46.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "ts-loader": "^8.0.14",
    "typescript": "^4.1.3",
    "vscode-codicons": "0.0.17",
    "vscode-test": "^1.5.0",
    "webpack": "^5.19.0",
    "webpack-cli": "^4.4.0"
  },
  "dependencies": {
    "@ronan-try/cli-service": "^1.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ronan-try/ro-vscod.git"
  },
  "bugs": {
    "url": "https://github.com/ronan-try/ro-vscode/issues"
  },
  "homepage": "https://github.com/ronan-try/ro-vscode",
  "license": "MIT"
}
